<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/tincan/db" VERSION="20150309" COMMENT="XMLDB file for Moodle report/tincan"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_tincan_grades" COMMENT="final course grades">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="name" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="full name"/>
        <FIELD NAME="course" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="course name"/>
        <FIELD NAME="type" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="item name"/>
        <FIELD NAME="score" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="score"/>
        <FIELD NAME="updated" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="time when graded"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
   </TABLES>
</XMLDB>
